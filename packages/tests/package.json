{
  "name": "@mcp-toolkit/tests",
  "version": "1.0.0",
  "description": "End-to-end tests for MCP Toolkit",
  "type": "module",
  "private": true,
  "scripts": {
    "test": "playwright test --trace on",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "report": "tsx generate-report.ts"
  },
  "devDependencies": {
    "@playwright/test": "^1.48.0",
    "@types/node": "^20.0.0",
    "tsx": "^4.7.0"
  }
}
