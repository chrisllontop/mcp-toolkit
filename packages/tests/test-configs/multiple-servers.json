{
  "mcpServers": {
    "filesystem-server": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "/Users/test"],
      "env": {
        "NODE_ENV": "production"
      }
    },
    "git-server": {
      "command": "uvx",
      "args": ["mcp-server-git"],
      "env": {
        "GIT_AUTHOR_NAME": "Test User"
      }
    },
    "custom-binary": {
      "command": "/usr/local/bin/custom-mcp",
      "args": ["--debug"],
      "env": {
        "API_KEY": "multi_server_key"
      }
    }
  }
}
